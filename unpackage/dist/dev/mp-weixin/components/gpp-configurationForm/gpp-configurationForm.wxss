.gpp-cf {
  background: #F1F1F1;
  box-sizing: border-box;
}
.gpp-cf .gpp-cf-form {
    box-sizing: border-box;
    padding-bottom: 10px;
    background: #FFFFFF;
    margin-bottom: 10px;
}
.gpp-cf .gpp-cf-form .gpp-cf-title {
      position: relative;
      padding: 10px 10px 10px 30px;
      font-size: 16px;
}
.gpp-cf .gpp-cf-form .gpp-cf-title .titleBeforeOpen {
        -webkit-transition: all 0.3s ease;
        transition: all 0.3s ease;
        position: absolute;
        left: 15px;
        top: 13px;
        content: "";
        width: 4px;
        height: 16px;
        border-radius: 2px;
}
.gpp-cf .gpp-cf-form .gpp-cf-title .titleBeforeClose {
        -webkit-transition: all 0.3s ease;
        transition: all 0.3s ease;
        position: absolute;
        left: 8px;
        top: 19px;
        content: "";
        width: 16px;
        height: 4px;
        border-radius: 2px;
}
.gpp-cf .gpp-cf-form .gpp-cf-title:active {
      background-color: #f8f8f8;
}
.gpp-cf .gpp-cf-form .gpp-cf-content {
      margin: 10px;
      box-sizing: border-box;
      font-size: 14px;
      color: #777777;
}
.gpp-cf .gpp-cf-form .gpp-cf-content .gpp-cf-content-item {
        margin-bottom: 16px;
}
.gpp-cf .gpp-cf-form .gpp-cf-content .gpp-cf-content-item .item-name {
          margin-bottom: 3px;
}
.gpp-cf .gpp-cf-form .gpp-cf-content .gpp-cf-content-item .item-content {
          position: relative;
}
.gpp-cf .gpp-cf-form .gpp-cf-content .gpp-cf-content-item .item-content .item-ct-unit {
            position: absolute;
            top: 8px;
            right: 10px;
            font-size: 14px;
            color: #d5d5d5;
}
.gpp-cf .gpp-cf-form .hidden {
      display: none;
}
.gpp-cf .gpp-cf-form:last-child {
    margin-bottom: 0;
}
.gpp-cf .red {
    color: #f55347;
}
.gpp-cf .bottomShadow {
    box-shadow: 0 2px 4px #d0cfcf;
}
.gpp-cf .arrowBottom {
    top: 11px !important;
    content: " ";
    height: 8px;
    width: 8px;
    border-width: 0 1px 1px 0;
    border-color: #a0a0a0;
    border-style: solid;
    -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
            transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
}
.gpp-cf .fork {
    width: 14px;
    height: 14px;
    margin: auto;
    position: relative;
}
.gpp-cf .fork:before, .gpp-cf .fork:after {
    content: "";
    position: absolute;
    height: 14px;
    width: 1.5px;
    top: 3px;
    right: 4px;
    background: #a0a0a0;
}
.gpp-cf .fork:before {
    -webkit-transform: rotate(45deg);
            transform: rotate(45deg);
}
.gpp-cf .fork:after {
    -webkit-transform: rotate(-45deg);
            transform: rotate(-45deg);
}
.gpp-cf input, .gpp-cf .item-select {
    box-sizing: border-box;
    width: 100%;
    height: 36px;
    line-height: 34px;
    font-size: 14px;
    padding: 0 40px 0 8px;
    border: 1px solid #e8e5e6;
    border-radius: 8px;
}
.gpp-cf textarea {
    box-sizing: border-box;
    width: 100%;
    min-height: 90px;
    font-size: 14px;
    padding: 8px;
    border: 1px solid #e8e5e6;
    border-radius: 8px;
}
.gpp-cf .input-placeholder, .gpp-cf .textarea-placeholder {
    color: #d5d5d5;
}
.gpp-cf radio-group, .gpp-cf checkbox-group {
    margin-top: 8px;
}
.gpp-cf .item-radio, .gpp-cf .item-checkbox {
    margin-right: 14px;
}

