<view class="dayTable"><view class="header"><view class="month">{{month+"月"}}</view><view class="week"><lx-calendar vue-id="f132097c-1" bind:__l="__l"></lx-calendar></view></view><block wx:if="{{allDay}}"><view class="allDay"><view class="title">全天</view><block wx:for="{{allDay}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="allDay_box" style="{{(item.style)}}"><view class="allDay_name">{{item.name}}</view></view></block></view></block><scroll-view class="calendar_body" style="{{(scrollHeight)}}" scroll-y="{{isScroll}}" scroll-top="{{scrollTop}}" data-event-opts="{{[['scroll',[['scroll',['$event']]]]]}}" bindscroll="__e"><view data-event-opts="{{[['tap',[['touchSt',['$event']]]]]}}" class="calendar_table" bindtap="__e"><block wx:for="{{defaultList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-ref="{{index}}" class="{{['cal_tr vue-ref-in-for',item.trClass]}}" style="{{'height:'+(unitHeight+'px;'+item.hidClass)+';'}}"><view class="left_time">{{item.timeTitle}}</view><view class="right_content"></view></view></block></view><view class="calendar_meeting"><block wx:for="{{meetingList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['showDetail',[index,'$event']]]]]}}" class="{{['meeting_a',item.bgClass]}}" style="{{(item.style)}}" bindtap="__e"><view class="create_content_box"><view class="meeting_content_name">{{item.name}}</view></view></view></block><block wx:if="{{meetingDetail}}"><view class="{{['meeting_detail',meetingDetail.class]}}" style="{{(meetingDetail.style)}}" id="myId"><view class="time">{{meetingDetail.time}}</view><view class="name">{{meetingDetail.name}}</view><view class="detalied">{{meetingDetail.detail}}</view><button class="btn" type="warn" data-event-opts="{{[['tap',[['deleteSchedule',['$0'],['meetingDetail.name']]]]]}}" bindtap="__e">删除</button></view></block><block wx:if="{{isCreate}}"><view data-event-opts="{{[['tap',[['creat']]]]}}" class="meeting_create" style="{{(createMeet.style)}}" bindtap="__e"><view class="radius_first radius"><view></view></view><view class="radius_second radius"><view></view></view><view class="meeting_left_time" style="{{(createMeet.trueStyle)}}"><view class="left_Time_show ">{{createMeet.startTimeShow}}</view><view class="left_Time_show endTimeText">{{createMeet.endTimeShow}}</view></view><view class="create_content_box"><text class="meeting_content">{{createMeet.name}}</text></view></view></block></view><block wx:if="{{isToday==0}}"><view class="time_now" style="{{(nowTime.line)}}"><view class="left_text_red">{{nowTime.text}}</view><view class="left_text_radio"></view><view class="time_line"></view></view></block></scroll-view></view>